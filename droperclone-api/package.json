  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/main.ts",
    "dev:track": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/tracker.ts",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -- -d migrations",
    "migration:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -- -d migrations",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.15.0",
